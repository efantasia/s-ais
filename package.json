  "dependencies": {
    "body-parser": "1.18.2",
    "cheerio": "0.22.0",
    "express": "4.16.0",
    "method-override": "2.3.10",
    "moment": "2.19.3",
    "nib": "1.1.2",
    "pg-promise": "5.9.2",
    "pug": "2.0.0-rc.2",
    "request": "2.81.0",
    "request-promise-native": "1.0.3",
    "sendgrid": "^2.0.0",
    "stylus": "0.54.5",
    "xml2js": "0.4.17",
    "snyk": "^1.41.1"
  },
  "devDependencies": {
    "ava": "0.17.0",
    "coveralls": "2.11.12",
    "nyc": "10.0.0",
    "proxyquire": "1.8.0",
    "sinon": "2.3.5",
    "xo": "0.17.0"
  },
  "xo": {
    "ignore": "public/js/*.js"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
