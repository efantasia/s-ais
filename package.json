{
  "name": "sAIS",
  "version": "1.0.1",
  "description": "Server-AIS self-reporting service for DeLorme inReach/Explorer devices",
  "license": "GPL",
  "repository": "infinitedescent/s-ais",
  "author": {
    "name": "Hays Clark",
    "email": "hays@infinitedescent.com",
    "url": "infinitedescent.com"
  },
  "bin": "bin/sais",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "start": "node server.js",
    "test": "xo && nyc ava",
